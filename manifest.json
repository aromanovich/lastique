{
  "manifest_version": 2,
  "name": "Lastique",
  "version": "0.1.1",
  "default_locale": "en",
  "description": "__MSG_extensionDescription__",
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["libs/zepto.min.js", "libs/md5.js", "lastfm.js",
                "shared_constants.js", "background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_end",
      "js": ["injector.js"]
    }
  ],
  "web_accessible_resources": ["shared_constants.js", "connectors/utils.js", "connectors/vk.js",
                               "connectors/youtube.js"]
}
