{
  "name": "Lastique",
  "version": "0.1",
  "background": {"scripts": ["zepto.min.js", "md5.js", "lastfm.js", "background.js"]},
  "description": "Last.fm scrobbler for vk.com",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs", "storage", "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "run_at": "document_end",
      "js": ["injector.js"]
    }
  ],
  "manifest_version": 2,
  "web_accessible_resources": ["connector.js"]
}
