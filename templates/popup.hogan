<h1>{{i18n.hello}}, <a href="http://www.last.fm/user/{{username}}">{{username}}</a>!</h1>
{{#isNothingToShow}}
  {{i18n.isNothingToShow}}
{{/isNothingToShow}}

{{#nowPlaying}}
  <div id="now-playing" class="song">
    {{> song}}
  </div>
{{/nowPlaying}}

<ul id="last-scrobbled">
  {{#lastScrobbled}}
    <li class="song">
      {{> song}}
    </li>
  {{/lastScrobbled}}
</ul>
